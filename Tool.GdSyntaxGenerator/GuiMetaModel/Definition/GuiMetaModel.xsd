	</xs:complexType>
	<xs:element name="GridLayoutInfo" substitutionGroup="LayoutInfo">
		<xs:annotation>
			<xs:documentation>A grid layout</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="LayoutInfo">
					<xs:attribute name="Row" default="0">
						<xs:simpleType>
							<xs:restriction base="xs:int">
								<xs:minInclusive value="0"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
					<xs:attribute name="Col" default="0">
						<xs:simpleType>
							<xs:restriction base="xs:int">
								<xs:minInclusive value="0"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
					<xs:attribute name="RowSpan" default="1">
						<xs:simpleType>
							<xs:restriction base="xs:int">
								<xs:minInclusive value="1"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
					<xs:attribute name="ColSpan" default="1">
						<xs:simpleType>
							<xs:restriction base="xs:int">
								<xs:minInclusive value="1"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
					<xs:attribute name="ColMode" default="Fill">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="Fixed"/>
								<xs:enumeration value="Fill"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
					<xs:attribute name="Width" type="xs:int" default="0"/>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="GridLayoutConstraint">
		<xs:attribute name="Mode" default="Variable">
			<xs:annotation>
				<xs:documentation>Mode definiert die Ausbreitung der Zeile/Spalte. Fixed heisst fester Wert, Variable heisst, dass sich der Wert bei Größenänderung anpasst.</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="Fixed"/>
					<xs:enumeration value="Variable"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="Value" type="xs:int" use="optional" default="100">
			<xs:annotation>
				<xs:documentation>Optionaler Wert, der den Mode genauer beschreibt. Bei Fixed gibt Value den absoluten Wert in Pixel an. Bei Variable gibt Value den Wert im Verhältnis an (z.B. in Prozent).</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
