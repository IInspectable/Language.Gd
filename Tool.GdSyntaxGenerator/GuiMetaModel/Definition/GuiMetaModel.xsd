					</xs:simpleType>
				</xs:attribute>
				<xs:attribute name="EnableLizenz" type="xs:string"/>
				<xs:attribute name="VisibleLizenz" type="xs:string"/>
				<xs:attribute name="MaxDropDownItems" type="xs:int" use="optional" default="0"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
